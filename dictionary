(kanji 一
    (kun
        (primary-reading
            (primary-spelling 一つ)
            *ひと*つ
            "one"
            (related-reading
                (primary-spelling 一人)
                *ひと*り
                "one person")))
    (on
        (primary-reading
            (primary-spelling 一)
            *イチ*
            "one")
        (secondary-reading
            (primary-spelling 唯一)
            ユイ*イツ*
            "sole")))
(kanji 二
    (kun
        (primary-reading
            (primary-spelling 二つ)
            *ふた*つ
            "two"
            (related-reading
                (primary-spelling 二人)
                *ふた*り
                "two people")
            (related-reading
                (primary-spelling 二日)
                *ふつ*か
                "second day")))
    (on
        (primary-reading
            (primary-spelling 二)
            *ニ*
            "two")
        (secondary-reading
            (primary-spelling 不二)
            フ*ジ*
            "two sides of the same coin")))
