        (primary-reading
            (secondary-spelling 生きる (rare 活きる))
            *い*きる
            "to live"
            (related-reading
                (secondary-spelling 生かす (common 活かす))
                *い*かす
                "to let live")
            (related-reading
                (secondary-spelling 生ける (common 活ける))
                *い*ける
                "to arrange in a vase")))
    (on
        (primary-reading
            (primary-spelling 生活)
            セイ*カツ*
            "life")))
