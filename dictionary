(kanji-split 中
    (part "kun"
        (kun
            (primary-reading
                (primary-spelling 中)
                *なか*
                "inside"
                (variant-reading
                    (primary-spelling 葦原中国)
                    あしはらの*なかつ*くに
                    "Japan"
                    (special genitive)))
            (secondary-reading
                (secondary-kanji-spelling うち (common 内) (rare 中))
                *うち*
                "inside")
            (secondary-reading
                (secondary-spelling 当たる (rare 中る))
                *あた*る
                "to hit"
                (variant (rare 中たる))
                (related-reading
                    (secondary-spelling 当てる (rare 中てる))
                    *あ*てる
                    "to hit"))))
    (part "on"
        (on
            (primary-reading
                (primary-spelling 中心)
                *チュウ*シン
                "center")
            (primary-reading
                (primary-spelling 中国)
                *チュウ*ゴク
                "China")
            (secondary-reading
                (primary-spelling 世界中)
                セカイ*ジュウ*
                "around the world"))))
(kanji-split 上
    (part "kun1"
        (kun
            (primary-reading
                (primary-spelling 上)
                *うえ*
                "up"
                (related-reading
                    (primary-spelling 上着)
                    *うわ*ぎ
                    "coat"))
            (primary-reading
                (primary-spelling 上がる)
                *あ*がる
                "to rise"
                (variant (rare 上る))
                (related-reading
                    (primary-spelling 上げる)
                    *あ*げる
                    "to raise"))))
    (part "on"
        (on
            (primary-reading
                (primary-spelling 向上)
                コウ*ジョウ*
                "rise")
            (secondary-reading
                (primary-spelling 上人)
                *ショウ*ニン
                "holy man")
            (secondary-reading
                (primary-spelling 上海)
                *シャン*ハイ
                "Shanghai"
                (special chinese))))
    (part "kun2"
        (kun
            (secondary-reading
                (primary-spelling 川上)
                かわ*かみ*
                "upstream")
            (secondary-reading
                (secondary-spelling 登る (common 上る))
                *のぼ*る
                "to climb, to rise"
                (related-reading
                    (primary-spelling 上せる)
                    *のぼ*せる
                    "to raise")))))
