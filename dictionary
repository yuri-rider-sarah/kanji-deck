        (primary-reading
            (primary-spelling 曜日)
            *ヨウ*ビ
            "day of the week")))
(kanji 建
    (kun
        (primary-reading
            (primary-spelling 建つ)
            *た*つ
            "to be built"
            (related-reading
                (primary-spelling 建てる)
                *た*てる
                "to build")))
    (on
        (primary-reading
            (primary-spelling 建築)
            *ケン*チク
            "construction")
        (secondary-reading
            (primary-spelling 建立)
            *コン*リュウ
            "construction")))
