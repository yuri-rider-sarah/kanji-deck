(kanji-split 乾
    (part "kun"
        (kun
            (primary-reading
                (primary-spelling 乾く)
                *かわ*く
                "to dry"
                (related-reading
                    (primary-spelling 乾かす)
                    *かわ*かす
                    "to dry"))
            (secondary-reading
                (secondary-spelling 干す (rare 乾す))
                *ほ*す
                "to dry"
                (related-reading
                    (secondary-spelling 干る (rare 乾る))
                    *ひ*る
                    "to dry")
                (related-reading
                    (primary-kanji-spelling 干からびる (common 干乾びる))
                    ひ*から*びる
                    "to dry up"))
            (secondary-reading
                (primary-spelling 乾拭き)
                *から*ぶき
                "wiping with a dry cloth")))
    (part "on"
        (on
            (primary-reading
                (primary-spelling 乾燥)
                *カン*ソウ
                "dryness")
            (secondary-reading
                (primary-spelling 乾坤)
                *ケン*コン
                "heaven and earth"))))
